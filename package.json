{
  "name": "@vforsh/bnn",
  "version": "0.1.1",
  "description": "CLI for generating and editing images with Gemini API",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "bnn": "bin/bnn"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "bun test",
    "cmd": "bun run bin/bnn"
  },
  "keywords": [
    "gemini",
    "ai",
    "image-generation",
    "cli"
  ],
  "author": "Vladislav Forsh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vforsh/bnn.git"
  },
  "homepage": "https://github.com/vforsh/bnn#readme",
  "bugs": {
    "url": "https://github.com/vforsh/bnn/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "nanoid": "^5.1.6",
    "ora": "^9.1.0",
    "slugify": "^1.6.6",
    "smol-toml": "^1.6.0",
    "zod": "^4.3.6"
  }
}
